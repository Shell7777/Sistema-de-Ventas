<h2>IniciarSession</h2>

<form action="/Login/IniciarSession" method="post">
    <div class="form-group row">
        <label for="inputEmail3" class="col-sm-2 col-form-label">Nombre</label>
        <div class="col-sm-10">
            <input type="text" name="nombre" class="form-control" id="inputEmail3" placeholder="Nombre">
        </div>
    </div>
    <div class="form-group row">
        <label for="inputPassword3" class="col-sm-2 col-form-label">Password</label>
        <div class="col-sm-10">
            <input type="password" name="contrasenna" class="form-control" id="inputPassword3" placeholder="Password">
        </div>
    </div>
    <button class="btn btn-block" >Enviar</button>

</form>

